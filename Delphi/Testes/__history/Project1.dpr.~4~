program Project1;

uses
  Vcl.Forms,
  Inicio in 'Inicio.pas' {Inicio1},
  Cadastro in 'Cadastro.pas' {Form1};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TInicio1, Inicio1);
  Application.CreateForm(TForm1, Form1);
  Application.Run;
end.
